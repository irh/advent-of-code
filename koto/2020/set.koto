set_meta =
    @type: 'Set'
    @iterator: || self.data.keys()

    @meta contains: |value|
      self.data.contains_key value

    @meta insert: |value|
      self.data.insert value
      self

    @size: || size self.data
    @meta is_empty: || (size self) == 0

export make_set = |values|
  data = {}

  if values != null
    for value in values
      data.insert value

  {data}.with_meta set_meta

iterator.to_set = || make_set self
